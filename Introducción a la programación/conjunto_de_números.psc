// Crea un conjunto de numeros en PSeInt y mostrar alguno de los datos guardados.
Algoritmo conjunto_de_números
	Dimension conjunto(5)
	
	conjunto[1] <- 3
	conjunto[2] <- 6
	conjunto[3] <- 9
	conjunto[4] <- 12
	conjunto[5] <- 15
	
	Escribir "El valor en la posición 1 del conjunto es ", conjunto[1]
	Escribir "El valor en la posición 4 del conjunto es ", conjunto[4]
FinAlgoritmo